{
  "title": "SharpBlueprint.Client.Tests",
  "description": "Unit tests for SharpBlueprint .NET Client",
  "copyright": "Copyright (c) 2016 FairyCoder",
  "version": "0.1.0-*",

  "configurations": {
    "Release": {
      "buildOptions": {
        "warningsAsErrors": true
      }
    }
  },

  "dependencies": {
    "SharpBlueprint.Client": {
      "target": "project"
    }
  },

  "frameworks": {

    "netcoreapp1.0": {

      "dependencies": {
        "Microsoft.NETCore.App": {
          "type": "platform",
          "version": "1.0.0"
        },
        "xunit": "2.2.0-beta2-build3300",
        "dotnet-test-xunit": "2.2.0-preview2-build1029"
      }
    },

    "net451": {
      "dependencies": {
        "xunit": "2.1.0",
        "dotnet-test-xunit": "1.0.0-rc2-build10025"
      },
      "frameworkAssemblies": {
        "System.Runtime": {
          "type": "build"
        }
      }
    }

  },

  "testRunner": "xunit"
}