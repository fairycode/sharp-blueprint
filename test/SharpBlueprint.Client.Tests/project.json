{
  "title": "SharpBlueprint.Client.Tests",
  "description": "Unit tests for SharpBlueprint .NET Client",
  "copyright": "Copyright (c) 2016 FairyCoder",
  "version": "0.1.0-*",

  "frameworks": {
    "netstandard1.6": {
      "buildOptions": {
        "debugType": "portable",
        "define": [
          "NETCORE"
        ]
      },
      "imports": "dnxcore50",
      "dependencies": {
        "NETStandard.Library": "1.6.0"
      }
    },
    "net452": {
      "frameworkAssemblies": {
        "System": {
          "type": "build"
        },
        "System.Core": {
          "type": "build"
        },
        "System.Data": {
          "type": "build"
        },
        "System.Net.Http": {
          "type": "build"
        },
        "System.Web": {
          "type": "build"
        }
      },
      "dependencies": {
        "Newtonsoft.Json": "9.0.1",
        "xunit": "2.1.0",
        "xunit.abstractions": "2.0.0",
        "xunit.assert": "2.1.0",
        "xunit.core": "2.1.0",
        "xunit.extensibility.core": "2.1.0",
        "xunit.extensibility.execution": "2.1.0"
      }
    },
    "net35": {
      "buildOptions": {
        "debugType": "portable",
        "define": [
          "NET35"
        ]
      },
      "frameworkAssemblies": {
        "System": {
          "type": "build"
        },
        "System.Data": {
          "type": "build"
        },
        "System.Web": {
          "type": "build"
        }
      },
      "dependencies": {
        "Newtonsoft.Json": "9.0.1",
        "NUnit": "3.4.1"
      }
    }
  }
}
