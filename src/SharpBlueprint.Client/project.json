{
  "title": "SharpBlueprint.Client",
  "version": "0.1.0-*",
  "description": "SharpBlueprint .NET client library",
  "copyright": "Copyright (c) 2016 FairyCoder",
  "authors": [
    "FairyCoder"
  ],

  "packOptions": {
    "summary": "Blueprint for C# projects with support for .NET 3.5, 4.5 and .NET Core platforms.",
    "licenseUrl": "https://github.com/fairycode/sharp-blueprint/blob/master/LICENSE.md",
    "requireLicenseAcceptance": false,
    "tags": [
      "C#",
      "Blueprint"
    ],
    "repository": {
      "type": "git",
      "url": "https://github.com/fairycode/sharp-blueprint"
    }
  },

  "configurations": {
    "Release": {
      "buildOptions": {
        "warningsAsErrors": true
      }
    }
  },

  "frameworks": {
    "netstandard1.6": {
      "buildOptions": {
        "debugType": "portable",
        "define": [
          "NETCORE"
        ]
      },
      "imports": "dnxcore50",
      "dependencies": {
        "NETStandard.Library": "1.6.0"
      }
    },
    "net452": {
      "frameworkAssemblies": {
        "System": {
          "type": "build"
        },
        "System.Core": {
          "type": "build"
        },
        "System.Data": {
          "type": "build"
        },
        "System.Net.Http": {
          "type": "build"
        },
        "System.Web": {
          "type": "build"
        }
      },
      "dependencies": {
        "Newtonsoft.Json": "9.0.1"
      }
    },
    "net35": {
      "buildOptions": {
        "debugType": "portable",
        "define": [
          "NET35"
        ]
      },
      "frameworkAssemblies": {
        "System": {
          "type": "build"
        },
        "System.Data": {
          "type": "build"
        },
        "System.Web": {
          "type": "build"
        }
      },
      "dependencies": {
        "Newtonsoft.Json": "9.0.1"
      }
    }
  }
}
